#!/bin/bash

echo "Setting up environment variables"
if [[ $TRAVIS_BRANCH == 'staging' ]]; then
    export AWS_ENV=potential-fiesta-staging
    export ZIP_FILE=potential-fiesta-staging-$(git rev-parse HEAD).zip
elif [[ $TRAVIS_BRANCH == 'master' ]]; then
    export AWS_ENV=potential-fiesta-production
    export ZIP_FILE=potential-fiesta-production-$(git rev-parse HEAD).zip
else
    exit 0
fi

echo "ENV=${AWS_ENV}"
echo "ZIP_FILE=${ZIP_FILE}"